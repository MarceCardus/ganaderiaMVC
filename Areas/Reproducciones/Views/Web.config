<?xml version="1.0"?>
<configuration>
	<configSections>
		<sectionGroup name="system.web.webPages.razor"
					  type="System.Web.WebPages.Razor.Configuration.RazorWebSectionGroup, System.Web.WebPages.Razor">
			<section name="host"
					 type="System.Web.WebPages.Razor.Configuration.HostSection, System.Web.WebPages.Razor"
					 requirePermission="false" />
			<section name="pages"
					 type="System.Web.WebPages.Razor.Configuration.RazorPagesSection, System.Web.WebPages.Razor"
					 requirePermission="false" />
		</sectionGroup>
	</configSections>

	<system.web.webPages.razor>
		<host factoryType="System.Web.Mvc.MvcWebRazorHostFactory, System.Web.Mvc" />
		<pages pageBaseType="System.Web.Mvc.WebViewPage">
			<namespaces>
				<add namespace="System.Web.Mvc" />
				<add namespace="System.Web.Mvc.Ajax" />
				<add namespace="System.Web.Mvc.Html" />
				<add namespace="System.Web.Optimization" />
				<add namespace="System.Web.Routing" />
				<add namespace="ganaderiaMVC" />
				<add namespace="ganaderiaModel" />
			</namespaces>
		</pages>
	</system.web.webPages.razor>

	<appSettings>
		<add key="webpages:Enabled" value="false" />
	</appSettings>

	<system.webServer>
		<handlers>
			<remove name="BlockViewHandler"/>
			<add name="BlockViewHandler" path="*" verb="*" preCondition="integratedMode" type="System.Web.HttpNotFoundHandler" />
		</handlers>
	</system.webServer>

	<system.web>
		<compilation>
			<assemblies>
				<add assembly="System.Web.Mvc" />
				<add assembly="System.Web.WebPages" />
				<add assembly="System.Web.WebPages.Razor" />
				<add assembly="System.Web.Razor" />
				<add assembly="System.Web.Helpers" />
				<add assembly="System.Web.Optimization" />
			</assemblies>
		</compilation>
	</system.web>
</configuration>
